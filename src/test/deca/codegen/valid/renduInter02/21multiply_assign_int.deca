// Description:
//      Opérateur binaire de multiplication (symbole "*")
//      => Cas spécifique avec utilisation du fois lors de l'affectation d'une variable de type int
//
// Resultats:
//      x=280
//      y=7
//      z=-40

{
    int t = 1;
    int y;
    int z;
    int x;
    y = 7 * t;
    z = 5 * -8;
    x = y * (-z);
    println("x=", x);
    println("y=", y);
    println("z=", z);
}