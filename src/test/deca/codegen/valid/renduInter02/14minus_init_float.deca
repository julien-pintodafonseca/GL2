// Description:
//      Opérateur binaire moins (symbole "-")
//      => Cas spécifique avec utilisation du moins lors de l'initialisation d'une variable de type float
//
// Resultats:
//      x=-7.2
//      y=6
//      z=4.2

{
    float u = 1.2;
    float y = 7.2 - u; // soustraction de deux nombres flottants
    float z = 1 - -3.2; // soustraction d'un nombre entier et d'un nombre flottant négatif
    int t = 3;
    float x = - t - z; // soustraction d'une variable de type int et de la négation d'une variable flottante
    println("x=", x);
    println("y=", y);
    println("z=", z);
}