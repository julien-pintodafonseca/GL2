// Description:
//      Opérateur binaire de multiplication (symbole "*")
//      => Cas spécifique avec utilisation du fois lors de l'affectation d'une variable de type float
//
// Resultats:
//      x=9.6
//      y=8.64
//      z=-3.2

{
    float u = 1.2;
    float y;
    float z;
    int t = 3;
    float x;
    y = 7.2 * u; // multiplication de deux nombres flottants
    z = 1 * -3.2; // multiplication d'un nombre entier et d'un nombre flottant négatif
    x = t * (-z); // multiplication d'une variable de type int et de la négation d'une variable flottante
    println("x=", x);
    println("y=", y);
    println("z=", z);
}