// Description:
//      Opérateur binaire plus (symbole "+")
//      => Cas spécifique avec utilisation du plus lors de l'affichage (println) du résultat entier d'une somme
//
// Resultats:
//      y=8
//      z=-3
//      x=11

{
    int t = 1;
    int y;
    int z;
    println("y=", y = 7 + t);
    println("z=", z = 5 + -8);
    println("x=", y + (-z));
}