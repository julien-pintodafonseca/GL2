// Description:
//      Opérateur binaire de division (symbole "/")
//      => Cas spécifique avec utilisation du divisé lors de l'affectation d'une variable de type float
//
// Resultats:
//      x=9.6
//      y=8.64
//      z=-3.2

{
    float u = 1.2;
    float y;
    float z;
    int t = 3;
    float x;
    y = 7.2 / u; // division de deux nombres flottants
    z = 1 / -3.2; // division d'un nombre entier et d'un nombre flottant négatif
    x = t / (-z); // division d'une variable de type int et de la négation d'une variable flottante
    println("x=", x);
    println("y=", y);
    println("z=", z);
}