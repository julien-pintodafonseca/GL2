// Description:
//      Opérateur unaire neg
//      => Cas spécifique avec utilisation de neg sur une expression neg
//
// Resultats:
//      x=1
//      y=-7.2
//      z=5

{
    float x;
    float y = (-7.2);
    int z = -(-5);
    x = --1;
    println("x=", -(-x));
    println("y=", --y);
    println("z=", z);
}