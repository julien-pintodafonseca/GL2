// Description:
//      Opérateur binaire de division (symbole "/")
//      => Cas spécifique avec utilisation du divisé lors de l'affichage (println) du résultat entier d'une division
//
// Resultats:
//      y=7
//      z=-2
//      x=3

{
    int t = 1;
    int y;
    int z;
    println("y=", y = 7 / t);
    println("z=", z = 10 / -5);
    println("x=", y / (-z));
}