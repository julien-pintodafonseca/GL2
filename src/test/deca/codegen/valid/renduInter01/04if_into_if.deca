// Description:
//      tests if_else : boucle if_else avec une branche "if", dans laquelle on retrouve une boucle
//                      if_else (branches "if", "else if" et "else"), et une branche "else"
// Resultats:
//      x > 0 : 3
//      x = 3

{
    int x = 3;
    if ( x > 0 ) {
        println("x > 0 : ", x);
        if ( x == 1)
        {
            println("x = 1 : ", x);
        }
        else if (x == 2) {
            println("x = 3 : ", x);
        }
        else
        {
            println(" x = ", x);
        }
    } else {
        println("x < 0 : ", x);
    }

}
