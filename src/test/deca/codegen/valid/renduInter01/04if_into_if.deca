// Description:
//      Test d'instruction conditionnelle if...else_if...else imbriquÃ©e dans un if...else
//      Conditions (variable == entier), (variable > entier) et (variable < entier)
//
// Resultats:
//      x > 0 : 3
//      x = 3

{
    int x = 3;
    if ( x > 0 ) {
        println("x > 0 : ", x);
        if ( x == 1)
        {
            println("x = 1 : ", x);
        }
        else if (x == 2) {
            println("x = 3 : ", x);
        }
        else
        {
            println("x = ", x);
        }
    } else {
        println("x < 0 : ", x);
    }

}
