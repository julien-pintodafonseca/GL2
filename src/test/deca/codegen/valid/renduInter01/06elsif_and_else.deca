// Description:
//      Test d'instruction conditionnelle if...else_if...else_if...else
//      Conditions (variable == entier)
//
// Resultats:
//      hello else : 7

{
    int x = 7;
    if ( x == 0 ) {
        println("hello if", x);
    } else if (x == 3) {
        println("hello else_if1 : ", x);
    } else if (x == 4) {
        println("hello else_if2 : ", x);
    } else {
        println("hello else : ", x);
    }

}
