// Description:
//      test if_else : boucle if_else avec une branche "if", dans laquelle on retrouve une boucle
//                      if_else (branches "if" et "else if"), et une branche "else if"
// Resultats:
//      x > 0 : 3

{
    int x = 3;
    if ( x > 0 ) {
        println("x > 0 : ", x);
        if ( x == 1)
        {
            println("x = 1 : ", x);
        }
        else if (x == 2) {
            println("x = 2 : ", x);
        }
    } else if ( x < 0) {
        println("x < 0 : ", x);
    }

}
