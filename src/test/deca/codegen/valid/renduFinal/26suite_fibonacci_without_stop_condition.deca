// Description:
//      Procédure récursive divergent
//      => Test de débordement de pile
//
// Resultats:
//      x=8


class A {
    int suiteFibo(int n) {
        // fibo (n) = fibo(n - 1) + fibo(n - 2)
            return suiteFibo(n-1) + suiteFibo(n-2);
    }
}

{
    A a = new A();
    println("x=", a.suiteFibo(7));
}
