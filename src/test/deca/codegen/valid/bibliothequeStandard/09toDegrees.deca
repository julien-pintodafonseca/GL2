#include "Math.decah"

{
    Math m = new Math();

    println(m._toDegrees(0));                           // 0.00000e+00;
    println(m._toDegrees(0.0175));                      // 1.00000e+00;
    println(m._toDegrees(0.0873));                      // 5.00000e+00;
    println(m._toDegrees(0.1745));                      // 1.00000e+01;
    println(m._toDegrees(0.2618));                      // 1.50000e+01;
    println(m._toDegrees(0.3491));                      // 2.00000e+01;
    println(m._toDegrees(0.4363));                      // 2.50000e+01;
    println(m._toDegrees(0.5236));                      // 3.00000e+01;
    println(m._toDegrees(0.6109));                      // 3.50000e+01;
    println(m._toDegrees(0.6981));                      // 4.00000e+01;
    println(m._toDegrees(m._getPI()/4));                // 4.50000e+01;
    println(m._toDegrees(m._getPI()/3));                // 6.00000e+01;
    println(m._toDegrees(m._getPI()/2));                // 9.00000e+01;
    println(m._toDegrees(m._getPI()/1.5));              // 1.20000e+02;
    println(m._toDegrees(m._getPI()));                  // 1.80000e+02;
    println(m._toDegrees(m._getPI()*1.25));             // 2.25000e+02;
    println(m._toDegrees(m._getPI()*1.5));              // 2.70000e+02;
    println(m._toDegrees(m._getPI()*1.5+0.5236*2));     // 3.30000e+02;
    println(m._toDegrees(m._getPI()*2));                // 3.60000e+02;
    println(m._toDegrees(-0.2618));                     // -1.50000e+01;
    println(m._toDegrees(-(m._getPI()/2)));             // -9.00000e+01;
    println(m._toDegrees(-(m._getPI()/1.5)));           // -1.20000e+02;
    println(m._toDegrees(-(m._getPI())));               // -1.80000e+02;
    println(m._toDegrees(-(m._getPI()*1.25)));          // -2.25000e+02;
    println(m._toDegrees(-(m._getPI()*1.5)));           // -2.70000e+02;
    println(m._toDegrees(-(m._getPI()*1.5+0.5236*2)));  // -3.30000e+02;
}
